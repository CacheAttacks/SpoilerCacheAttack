coupon_prob <- function(balls, bins){
needed_balls <- 0
for(i in 1:bins){
next_ball_avg <- bins/(bins-i+1)
needed_balls <- needed_balls + next_ball_avg
if(needed_balls > balls)
return(i)
}
}
