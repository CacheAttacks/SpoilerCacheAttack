calc_prob_exact <- function(balls, bins, full_bins){
#[prod x=0 to full_bins-1 ((bins-x)/bins)]
res <- Reduce('*', lapply(1:(full_bins-1), function(x) (bins-x)/bins), init = 1)
if(balls > full_bins)
res <- res * (full_bins/bins)^(balls-full_bins)
return(res)
}
