#bins choose full_bins * full_bins! * S(balls,full_bins)
combs <- choose(bins, full_bins) * factorial(full_bins) *
as.numeric(gmp::Stirling2(balls, full_bins))
all_combs <- bins^balls
prob <- combs/all_combs
return(prob)
}
