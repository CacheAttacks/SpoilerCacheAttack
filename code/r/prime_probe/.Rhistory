if(s<10^-10) {
power <- 50 # e^50 is already astronomical. sizes beyond that dont matter
}
else {
power <- min(50, (epsilon*1)/(2*sens)) # score = 1
}
}
else {
power <- 0 #score 0
}
weighted[i] <- exp(power)
}
sum_weighted <- sum(weighted)
